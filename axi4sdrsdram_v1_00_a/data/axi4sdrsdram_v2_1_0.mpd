PORT S_SDRAMWE = "", DIR = O
PORT S_SDRAMBA = "", DIR = O, VEC = [(C_SDRAMCLOG2BANKCOUNT-1):0]
PORT S_SDRAMA = "", DIR = O, VEC = [(C_SDRAMABITSIZE-1):0]
